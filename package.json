{
  "name": "egregore-dapp",
  "version": "1.1.2",
  "private": true,
  "author": "team egregore",
  "repository": "https://github.com/Egregore-ai",
  "main": "electron/main.js",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev --turbopack",
    "dev-debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev-https": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate --no-hints",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "desktop:dev": "concurrently -r -k \"next dev --turbopack -p 3005\" \"wait-on http-get://127.0.0.1:3005 && cross-env ELECTRON_START_URL=http://127.0.0.1:3005 electron .\"",
    "desktop:build": "npm run build && electron-builder -mwl",
    "desktop:build:mac": "npm run build && electron-builder --mac",
    "desktop:build:win": "npm run build && electron-builder --win",
    "desktop:build:linux": "npm run build && electron-builder --linux",
    "desktop:build:mac:x64": "npm run build && electron-builder --mac --x64",
    "desktop:build:mac:arm64": "npm run build && electron-builder --mac --arm64",
    "desktop:build:win:x64": "npm run build && electron-builder --win --x64",
    "desktop:build:win:ia32": "npm run build && electron-builder --win --ia32",
    "desktop:build:linux:x64": "npm run build && electron-builder --linux --x64",
    "desktop:build:linux:arm64": "npm run build && electron-builder --linux --arm64"
  },
  "prisma": {
    "schema": "src/server/prisma/schema.prisma"
  },
  "dependencies": {
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.18.0",
    "@mui/joy": "^5.0.0-beta.52",
    "@next/bundle-analyzer": "~15.1.8",
    "@prisma/client": "~5.22.0",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@tanstack/react-query": "^5.85.5",
    "@trpc/client": "^11.4.4",
    "@trpc/next": "^11.4.4",
    "@trpc/react-query": "^11.4.4",
    "@trpc/server": "^11.4.4",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "browser-fs-access": "^0.38.0",
    "cheerio": "^1.1.2",
    "csv-stringify": "^6.6.0",
    "dexie": "^4.0.11",
    "dexie-react-hooks": "^1.1.7",
    "diff": "^8.0.2",
    "eventemitter3": "^5.0.1",
    "get-port": "^7.1.0",
    "idb-keyval": "^6.2.2",
    "mammoth": "^1.10.0",
    "nanoid": "^5.1.5",
    "next": "^15.6.0-canary.34",
    "nprogress": "^0.2.0",
    "pdfjs-dist": "5.4.54",
    "posthog-js": "^1.259.0",
    "posthog-node": "^5.6.0",
    "prismjs": "^1.30.0",
    "puppeteer-core": "^24.16.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.62.0",
    "react-markdown": "^10.1.0",
    "react-player": "^3.3.1",
    "react-resizable-panels": "^3.0.4",
    "react-timeago": "^8.3.0",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-mark-highlight": "^0.1.1",
    "remark-math": "^6.0.0",
    "sharp": "0.32.6",
    "superjson": "^2.2.2",
    "tesseract.js": "^6.0.1",
    "tiktoken": "^1.0.21",
    "turndown": "^7.2.0",
    "viem": "^2.36.0",
    "wagmi": "^2.16.8",
    "zod": "^4.0.16",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@posthog/nextjs-config": "1.1.0",
    "@types/diff": "^7.0.2",
    "@types/node": "^24.2.1",
    "@types/nprogress": "^0.2.3",
    "@types/prismjs": "^1.26.5",
    "@types/react": "19.1.9",
    "@types/react-csv": "^1.1.10",
    "@types/react-dom": "19.1.7",
    "@types/react-timeago": "^4.1.7",
    "@types/turndown": "^5.0.5",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron": "^37.4.0",
    "electron-builder": "^26.0.12",
    "electron-is-dev": "^3.0.1",
    "electron-serve": "^2.1.1",
    "eslint": "^9.33.0",
    "eslint-config-next": "~15.1.8",
    "prettier": "^3.6.2",
    "prisma": "~5.22.0",
    "typescript": "^5.9.2",
    "wait-on": "^8.0.4"
  },
  "engines": {
    "node": "^24.0.0 || ^22.0.0 || ^20.0.0"
  },
  "build": {
    "appId": "com.yourdomain.dappagi",
    "productName": "Egregore",
    "asar": true,
    "files": [
      "electron/**",
      ".next/**",
      "public/**",
      "package.json",
      "node_modules/**"
    ],
    "asarUnpack": [
      "node_modules/sharp/**"
    ],
    "directories": {
      "output": "release"
    },
    "artifactName": "${productName}-${version}-${os}-${arch}.${ext}",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "hardenedRuntime": true,
      "icon": "assets/icons/mac/icon.icns"
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "assets/icons/win/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "category": "Utility",
      "icon": "assets/icons/linux/icon.png"
    },
    "protocols": [
      {
        "name": "Egregore",
        "schemes": [
          "Egregore"
        ]
      }
    ]
  }
}
